{
  "name": "Softoo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "iosDebugBuild": "detox build --configuration ios.sim.debug",
    "iosDebugTest": "detox test --configuration ios.sim.debug",
    "iosDebugTestWithScreenShots": "detox test --configuration ios.sim.debug",
    "iosDebugTestWithVideo": "detox test --configuration ios.sim.debug --record-videos all",
    "iosReleaseTest": "detox test --configuration ios.sim.release",
    "iosReleaseTestWithScreenShots": "detox test --configuration ios.sim.release",
    "iosReleaseTestWithVideo": "detox test --configuration ios.sim.release --record-videos all",
    "androidDebugTestWithScreenShots": "detox test --configuration android.sim.debug --take-screenshots all",
    "androidDebugTestWithVideo": "detox test --configuration android.sim.debug --record-videos all",
    "androidReleaseTest": "detox test --configuration android.sim.release --take-screenshots all",
    "androidReleaseTestWithScreenShots": "detox test --configuration android.sim.release",
    "androidReleaseTestWithVideo": "detox test --configuration android.sim.release --record-videos all"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "axios": "^1.5.1",
    "react": "18.0.0",
    "react-native": "0.69.12",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^19.7.1",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-circus": "27.5.1",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
